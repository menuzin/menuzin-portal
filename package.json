{
  "name": "morina-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "setup:env": "node scripts/ensure-env.js",
    "db:push": "dotenv -e .env -- prisma db push",
